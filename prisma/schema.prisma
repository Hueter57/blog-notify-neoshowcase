generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}


model Schedule {
  id              Int      @id @default(autoincrement())
  status          String
  crowiPath       String
  channelId       String
  logChannelId    String
  reviewChannelId String
  title           String
  Tag             String
  startDate       DateTime
  blogDays        Int
  createdAt       DateTime @default(now())
}

model Admin {
  id     Int    @id @default(autoincrement())
  userid String @unique
}
